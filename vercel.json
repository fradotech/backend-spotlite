{
    "version": 2,
    "builds": [
      {
        "src": "main.ts",
        "use": "@vercel/node",
        "config": {
          "includeFiles": "**",
          "buildCommand": "tsc",
          "startCommand": "node dist/main.js"
        }
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "/$1"
      }
    ]
  }